    },
    "immutable_id": {
      "anyOf": [
        {
          "format": "objectid",
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "The immutable database ID of the entry.",
      "format": "uuid",
      "title": "Immutable ID"
    },
    "last_modified": {
      "anyOf": [
        {
          "format": "datetime",
          "type": "string"
        },
